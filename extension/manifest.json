{
  "manifest_version": 3,
  "name": "Iframe Injection Example",
  "version": "1.0",
  "permissions": ["tabs", "webNavigation", "scripting", "activeTab", "storage", "webRequest", "cookies", "downloads", "debugger"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "allFrames": true
    }
  ]
}
